import java.util.ArrayList;

public class JavaArrayList {
public static void main(String[] args) {
	// create array list
	ArrayList list = new ArrayList();
	list.add("Java");
	list.add(12.3);
	list.add(100);
	list.add('A');
	
	for(int i=0; i<list.size(); i++) {
		System.out.println(list.get(i));
	}
}
}
