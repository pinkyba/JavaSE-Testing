package CollectionAssignment;

import java.util.*;

public class CollectionWithSet {
	
	public static void main(String[] args) {
		Set<Student> hashSet = new HashSet<Student>(); 
		
		Scanner sc = new Scanner(System.in);
		for(int i=0; i<3; i++) {
			Student s = new Student();
			System.out.print("Enter roll no: ");
			s.setRno(sc.nextInt());
			sc.nextLine();
			System.out.print("Enter name: ");
			s.setName(sc.nextLine());
			hashSet.add(s);
		}
		System.out.println("------Student List------");
		for(Student s: hashSet) {
			System.out.print("Roll no: "+s.getRno()+" -> ");
			System.out.println(s.getName());
			System.out.println();
		}
		int tmp;
		Student[] arr = hashSet.toArray( new Student[hashSet.size()] );
        for (int i=0; i<arr.length; i++) {
        	if(arr[i+1].getRno() < arr[i].getRno()) {
        		arr[i].setRno(arr[i+1]);
        		tmp = arr[i+1];
        		arr[i] = tmp;
        	}
        }
    } 
}
